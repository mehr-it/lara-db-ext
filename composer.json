{
  "name": "mehr-it/lara-db-ext",
  "type": "library",
  "description": "Extensions for laravel's database abstraction layer",
  "keywords": [
    "update joined",
    "insert models",
    "insert multiple models",
    "update multiple models",
    "laravel",
    "eloquent"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrIt\\LaraDbExt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MehrItLaraDbExtTest\\": "test/"
    }
  },
  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4|^8.4",
    "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MehrIt\\LaraDbExt\\Provider\\LaraDbExtServiceProvider"
      ]
    }
  }
}
